{
  "name": "unconfig",
  "version": "0.3.10",
  "packageManager": "pnpm@8.6.10",
  "description": "A universal solution for loading configurations.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/unconfig#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/unconfig.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/unconfig/issues"
  },
  "keywords": [
    "config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./presets": {
      "types": "./dist/presets.d.ts",
      "require": "./dist/presets.cjs",
      "import": "./dist/presets.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "dependencies": {
    "@antfu/utils": "^0.7.5",
    "defu": "^6.1.2",
    "jiti": "^1.19.1",
    "mlly": "^1.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@antfu/ni": "^0.21.5",
    "@types/node": "^20.4.5",
    "bumpp": "^9.1.1",
    "eslint": "^8.46.0",
    "esno": "^0.17.0",
    "lodash-es": "^4.17.21",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  }
}